<p id="notice"><%= notice %></p>

<h1>Listing Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apepat</th>
      <th>Apemat</th>
      <th>Tipdoc</th>
      <th>Numdoc</th>
      <th>Direccion</th>
      <th>Fecnac</th>
      <th>Tfno</th>
      <th>Celular</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nombre %></td>
        <td><%= cliente.apepat %></td>
        <td><%= cliente.apemat %></td>
        <td><%= cliente.tipdoc %></td>
        <td><%= cliente.numdoc %></td>
        <td><%= cliente.direccion %></td>
        <td><%= cliente.fecnac %></td>
        <td><%= cliente.tfno %></td>
        <td><%= cliente.celular %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
